/* yo-tabs*/
div.yo-tabs
{
	/*background-color: #636D76;
    border: 1px solid #494E52;
    padding: 8px;*/
   background-color:#AAA;
}
div.yo-tabs > ul
{
    padding: 7px 0 7px;
    overflow: visible;
}
div.yo-tabs > ul > li {
    display: inline;
    list-style: none outside none;
    overflow: visible;
}
div.yo-tabs > ul > li a {
    background-color: #888;
    /*background-image: url("../images/new/tab_off.jpg");*/
    background-position: center bottom;
    background-repeat: repeat-x;
    border: 1px solid #666;
    color: #FFF;
    font-family: 'Lucida Grande',Lucida,Arial,sans-serif;
    font-size: 10px;
    font-weight: bold;
    padding: 6px 14px;
    text-decoration: none;
    /*text-transform: uppercase;*/
}
div.yo-tabs > ul > li a:hover {
    background-color: #666;
    border-color: #444;
}
div.yo-tabs > ul > li a.active {
    background-color: #FFFFFF;
    background-image: url("../images/new/tab_on.jpg");
    background-position: center top;
    background-repeat: repeat-x;
    border-color: #464C54 #464C54 #FFFFFF;
    border-style: solid;
    border-width: 1px;
    color: #282E32;
}
div.yo-tabs > div {
    background-color: #FFFFFF;
    /*background-image: url("../images/new/content_bottom.jpg");*/
    background-position: center bottom;
    background-repeat: repeat-x;
    border-top: 1px solid #464C54;
}


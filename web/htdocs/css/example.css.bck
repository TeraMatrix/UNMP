/* host bar */
div.host_bar
{
	width: 100%;
	display: block;
	border-bottom: 1px solid #999999;
	border-left: 1px solid #B5B5B5;
	border-right: 1px solid #B5B5B5;
	background: none repeat scroll 0 0 #FAFAFA;
	height: 80px;
	font-size: 10px;
}

div.host_bar div#device_icon
{
	width: 50px;
	height: 50px;
	border: 0px none;
	margin: 15px 5px 15px 10px;
	float: left;
}
div.host_bar div#device_icon img
{
	border: opx none;
}
div.host_bar div.device_info
{
	width: 180px;
	height: 70px;
	margin: 5px;
	border-left: 1px solid #B5B5B5;
	color: #222222;
	float: left;
}
div.host_bar div.device_info2
{
	float: left;
	width: 170px;
	border-left: 0px none;
	border-right: 1px solid #D9D9D9;
}
div.host_bar div.device_info p.head
{
	/*color: #DD4B39;*/
	font-weight: bold;
	font-size: 11px;
	height: 15px;
	margin-bottom: 5px;
}

    background:	-webkit-gradient(linear, 0% 0%, 0% 100%, from(#ebebeb), to(#bdbdbd));
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#bdbdbd', endColorstr='#ebebeb');

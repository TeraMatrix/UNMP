}
div#extra_host_bar
{
	background-color: #F5F5F5;
}
div.bar-btn-div
{
	position: relative;
	float: right;
	vertical-align: top;
	margin: 26px 30px;
}
div.bar-btn-div a:active,div.bar-btn-div a.active 
{
	-moz-box-shadow: 0 1px 5px #CCCCCC inset;
	-webkit-box-shadow: 0 1px 5px #CCCCCC inset;
	box-shadow: 0 1px 5px #CCCCCC inset;
	background: -moz-linear-gradient(center top, #FAFAFA, #F1F1F1) repeat scroll 0 0 transparent;
	border-color: #BBBBBB;
	z-index: 2;
}
div.bar-btn-div a:hover
{
	background: -moz-linear-gradient(center top, #F8F8F8, #F1F1F1) repeat scroll 0 0 transparent;
	border-color:#A0A0A0;
	z-index: 1;
}
div.bar-btn-div a:active span,div.bar-btn-div a.active span,div.bar-btn-div a:hover span
{
	opacity:1.0;
	filter:alpha(opacity=100);
}
div.bar-btn-div a
{
	background: -moz-linear-gradient(center top, #FAFAFA, #FFFFFF) repeat scroll 0 0 transparent;
	border: 1px solid #C9C9C9;
	height: 27px;
	margin-left: -1px;
	text-align: center;
	vertical-align: middle;
	width: 32px;
    cursor: pointer;
    outline: medium none;
    text-decoration: none;
    display: inline-block;
    position: relative;
}
div.bar-btn-div a span
{
	background-color: #A2A2A77;
	height: 18px;
    width: 18px;
}
div.bar-btn-div a.ft
{
	-moz-border-radius: 2px 0 0 2px;
	-webkit-border-radius: 2px 0 0 2px;
	border-radius: 2px 0 0 px;
	margin-left: 0px;
}
div.bar-btn-div a.lt
{
	-moz-border-radius: 0 2px 2px 0;
	-webkit-border-radius: 0 2px 2px 0;
	border-radius: 0 2px 2px 0;
}
div.bar-btn-div a span
{
	display:inline-block;
	margin-top: 5px;
	width: 16px;
	height: 16px;
	opacity:0.8;
	filter:alpha(opacity=80);
}

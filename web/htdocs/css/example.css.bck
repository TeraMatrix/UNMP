*
{
	margin: 0px;
	padding: 0px;
	/*overflow: hidden;*/
}
body
{
	margin:0 auto;
	padding:0px;
	border:0px;
/*	background:#383838 none;*/
	font-family: 'Lucida Grande',Lucida,Arial,sans-serif;
	font-size: 10px;
	overflow:hidden;
	display:block;
	position:absolute;
	top:0;
	left:0;
	right:0;
	bottom:0;
	/*height:100%; hack for ie7 solved scroll bar issue*/
}
#page_header
{
	width: 100%;
	height: 40px;
	clear: both;
	position: relative;
	display: block;
	margin: 0px auto;
	padding: 0px;
	/*border-top: 1px solid #c9c9c9;
	border-bottom: 1px solid #a9a9a9;*/
	background: -moz-linear-gradient(0% 75% 90deg, #f9f9f9, #b9b9b9);  /* For Mozila Firefox */
	background:	-webkit-gradient(linear, 0% 0%, 0% 100%, from(#b9b9b9), to(#f9f9f9)); /* For Chrome */
	filter:  progid:DXImageTransform.Microsoft.gradient(startColorStr='#f9f9f9', EndColorStr='#b9b9b9'); /* For IE6 and IE7 */
	-ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorStr='#f9f9f9', EndColorStr='#b9b9b9')"; /* For IE8 */
}
#logo
{
	width: 151px;
	margin: 0px auto;
	padding: 0px;
	position: absolute;
	/*border-right: 1px solid #a9a9a9;*/
	top: 0;
	bottom: 0;
	background: url(../images/new/logo.png) no-repeat 9px;
}
#logo a
{
	color: #fff;
	text-indent: -1000px;
	display: block;
	height: 40px;
}
#page_title 
{
	left:217px;
	margin:0px;
	border:0px;
	position:absolute;
	padding:6px 12px;
	border-left:1px solid #fff;
	border-right:1px solid #a9a9a9;
	right:343px;
	top:0;
	bottom:0;
	font-weight:bold;
	font-size:10px;
	text-shadow:0px 1px 1px #000;
	display: block;
}
#page_title span 
{
	font-size:28px;
	color:#efd;
	text-shadow:0px 1px 5px #222;
	word-spacing:5px;
	letter-spacing:1px;
	font-family: 'Lucida Grande',Lucida,Arial,sans-serif;
}
#icons_div .icon 
{
	display:block;
	position:relative;
	float:right;
	height:100%;
	border-right:1px solid #fff;
	border-left:1px solid #a9a9a9;
	width:95px;
}

#icons_div a 
{
	text-decoration:none;
	color:#111;
	display:block;
	height:30px;
	padding:10px 10px 0 10px;
	text-shadow:0px 1px 1px #fff;
	font-size: 12px;
	text-align:center;
	text-transform:uppercase;
}
#icons_div .icon:hover 
{
	background: -moz-linear-gradient(10% 90% 90deg, #C23123, #93322A);
	background:	-webkit-gradient(linear, 0% 0%, 0% 100%, from(#93322A), to(#C23123));
 	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#93322A', endColorstr='#C23123');
	border-left:1px solid #d24133;
	border-right:1px solid #83221A;
}
#icons_div .icon:hover a 
{
	color:#fff;
	text-shadow:0px 1px 1px #AAA;
}
#icons_div .active 
{
	background: -moz-linear-gradient(10% 90% 90deg, #C23123, #93322A);
	background:	-webkit-gradient(linear, 0% 0%, 0% 100%, from(#93322A), to(#C23123));
 	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#93322A', endColorstr='#C23123');
	border-left:1px solid #d24133;
	border-right:1px solid #83221A;
	height:38px;
	border-bottom:1px solid #BDBDBD;
	border-top:1px solid #a3423A;
	z-index:101;
}
#icons_div .active a
{
	text-decoration:none;
	color:#fff;
	text-shadow:0px 1px 1px #c9c9c9;
	z-index:101;
	height:29px;
	background: -moz-linear-gradient(10% 90% 90deg, #C23123, #93322A);
	background:	-webkit-gradient(linear, 0% 0%, 0% 100%, from(#93322A), to(#C23123));
 	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#93322A', endColorstr='#C23123');
}

#tactical_view
{
	border-left:1px solid #a9a9a9;
	padding: 0px;
	height:40px;
	float: right;
	display:block;
